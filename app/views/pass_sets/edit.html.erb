<h1>Editing Pass Set</h1>

<%= render 'form' %>

<% if params[:location_id] != nil %>
	<%= link_to 'Show', [@location.user, @location, @pass_set] %> |
	<%= link_to 'Back', [@location.user, @location] %>
<% elsif params[:event_id] != nil %>
	<%= link_to 'Show', [@event.user, @event, @pass_set] %> |
	<%= link_to 'Back', [@event.user, @event] %>
<% end %>